s on ul, ol elements with a list role, which suggests default styling will be removed */\r\nul[role=\"list\"],\r\nol[role=\"list\"] {\r\n  list-style: none;\r\n}\r\n\r\n/* Set core root defaults */\r\nhtml:focus-within {\r\n  scroll-behavior: smooth;\r\n}\r\n\r\n/* Set core body defaults */\r\nbody {\r\n  min-height: 100vh;\r\n  text-rendering: optimizeSpeed;\r\n  line-height: 1.5;\r\n}\r\n\r\n/* A elements that don't have a class get default styles */\r\na:not([class]) {\r\n  text-decoration-skip-ink: auto;\r\n}\r\n\r\n/* Make images easier to work with */\r\nimg,\r\npicture {\r\n  max-width: 100%;\r\n  display: block;\r\n}\r\n\r\n/* Inherit fonts for inputs and buttons */\r\ninput,\r\nbutton,\r\ntextarea,\r\nselect {\r\n  font: inherit;\r\n}\r\n\r\n/* Remove all animations, transitions and smooth scroll for people that prefer not to see them */\r\n@media (prefers-reduced-motion: reduce) {\r\n  html:focus-within {\r\n    scroll-behavior: auto;\r\n  }\r\n\r\n  *,\r\n  *::before,\r\n  *::after {\r\n    animation-duration: 0.01ms !important;\r\n    animation-iteration-count: 1 !important;\r\n    transition-duration: 0.01ms !important;\r\n    scroll-behavior: auto !important;\r\n  }\r\n}\r\n\r\n// Google font import\r\n@import url(\"https://fonts.googleapis.com/css2?family=Nunito:ital,wght@0,400;0,700;1,700&family=Open+Sans:ital,wght@0,300;0,400;0,600;0,700;1,300;1,400;1,600;1,700&display=swap\");\r\n\r\n// Custom variables\r\n\r\n$font-header: \"Nunito\", sans-serif;\r\n$font-main: \"Open Sans\", sans-serif;\r\n\r\n$color-blue: #4d8cf5;\r\n$color-text-main: rgb(92, 92, 92);\r\n\r\n// Mixins\r\n\r\n@mixin flex-center {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n// Utility classes\r\n.primary-header {\r\n  font-family: $font-header;\r\n  color: #333;\r\n  font-size: 1.75rem;\r\n  line-height: 2rem;\r\n  font-weight: 700;\r\n  margin: 1.25rem auto 0.625rem;\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n}\r\n\r\n.secondary-header {\r\n  font-family: $font-main;\r\n  font-weight: 400;\r\n  font-size: 0.875rem;\r\n  font-style: italic;\r\n  text-align: center;\r\n  line-height: 1.75rem;\r\n  color: $color-text-main;\r\n}\r\n\r\n.uppercase {\r\n  text-transform: uppercase;\r\n}\r\n\r\n.blue {\r\n  background-color: $color-blue;\r\n}\r\n\r\n.white {\r\n  background-color: #fff;\r\n}\r\n\r\n.align-center {\r\n  text-align: center;\r\n}\r\n\r\n.button {\r\n  display: inline-block;\r\n  border: 0;\r\n  text-transform: uppercase;\r\n  cursor: pointer;\r\n  text-decoration: none;\r\n  line-height: inherit;\r\n  text-align: center;\r\n  border-radius: 0.3125em;\r\n  margin: 0.625em;\r\n  padding: 0.625em 1.875em;\r\n}\r\n\r\n// General styles\r\nbody {\r\n  font-family: \"Open Sans\", sans-serif;\r\n  color: #5c5c5c;\r\n  font-size: 0.9375rem;\r\n  line-height: 1.375rem;\r\n  font-weight: 400;\r\n  background-color: #fff;\r\n}\r\n\r\nh1 {\r\n  margin-top: 1.25rem;\r\n  margin-bottom: 0.625rem;\r\n  font-family: Nunito, sans-serif;\r\n  color: #333;\r\n  font-size: 2rem;\r\n  line-height: 2.75rem;\r\n  font-weight: 700;\r\n}\r\n\r\nh2 {\r\n  margin-top: 1.25rem;\r\n  margin-bottom: 0.625rem;\r\n  font-family: Nunito, sans-serif;\r\n  color: #333;\r\n  font-size: 1.75rem;\r\n  line-height: 2rem;\r\n  font-weight: 700;\r\n}\r\n\r\nh3 {\r\n  margin-top: 1.25rem;\r\n  margin-bottom: 0.625rem;\r\n  font-family: Nunito, sans-serif;\r\n  color: #333;\r\n  font-size: 1.25rem;\r\n  line-height: 1.75rem;\r\n  font-weight: 700;\r\n}\r\n\r\nh4 {\r\n  margin-top: 0.625rem;\r\n  margin-bottom: 0.3125rem;\r\n  font-family: Nunito, sans-serif;\r\n  color: #333;\r\n  font-size: 1rem;\r\n  line-height: 1.375rem;\r\n  font-weight: 700;\r\n}\r\n\r\np {\r\n  margin-bottom: 1.25rem;\r\n}\r\n\r\na {\r\n  -webkit-transition: all 300ms ease-in-out;\r\n  transition: all 300ms ease-in-out;\r\n  color: $color-blue;\r\n  font-weight: 600;\r\n  text-decoration: none;\r\n}\r\n\r\n// Sections\r\n\r\n// Sticky Nav\r\n.sticky-nav {\r\n  display: none;\r\n}\r\n\r\n// Hero Section\r\n\r\n.hero-section {\r\n  background-image: linear-gradient(\r\n      180deg,\r\n      rgba(0, 0, 0, 0.75),\r\n      rgba(0, 0, 0, 0.75)\r\n    ),\r\n    url(../images/hero.jpg);\r\n  background-position: 0px 0px, 50% 50%;\r\n  background-size: auto, cover;\r\n  background-repeat: repeat, no-repeat;\r\n\r\n  .hero-container {\r\n    min-height: 770px;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    .nav-container {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      width: 100%;\r\n      margin: 1rem 2rem;\r\n      padding: 3rem;\r\n      .mobile-nav-container {\r\n        .nav-toggle {\r\n          color: white;\r\n          i {\r\n            cursor: pointer;\r\n          }\r\n        }\r\n        .mobile-nav {\r\n          display: none;\r\n        }\r\n      }\r\n    }\r\n    .hero-content {\r\n      .hero-heading {\r\n        color: white;\r\n        margin: 1rem;\r\n        padding: 1rem;\r\n        font-size: 2rem;\r\n        line-height: 1.5;\r\n      }\r\n      .hero-text {\r\n        color: white;\r\n        line-height: 1.75rem;\r\n        margin: 1rem;\r\n        padding: 1rem;\r\n      }\r\n      .button-container {\r\n        @include flex-center;\r\n        flex-direction: column;\r\n      }\r\n    }\r\n    .arrow-container {\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"aff8b8860e8abd3abdd430a5460e073b","cacheData":{"env":{}}}